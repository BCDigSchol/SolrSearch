class SolrSearchFacetTableTest_GroupByElementSet
    extends SolrSearch_Test_AppTestCase
{


    /**
     * `findByName` should return return the facet with a given name.
     */
    public function testGroupByElementSet()
    {

        $facet = new SolrSearchFacet();
        $facet->label = 'facet';
        $facet->name  = 'facet';
        $facet->save();

        $retrieved = $this->facetTable->findByName('facet1');

    }


}
